{
  "presets": [
    [
      "@nrwl/react/babel",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ],
    [
      "babel-plugin-direct-import",
      { "modules": ["@mui/material", "@mui/icons-material", "@mui/lab"] }
    ]
  ]
}
